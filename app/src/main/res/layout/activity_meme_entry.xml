<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".MemeEntryActivity">


	<TextView
		android:id="@+id/textView2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Upload your favorite memes"
		android:textColor="@color/matteBlack"
		android:textSize="24sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.392"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.052" />

	<ImageView
		android:id="@+id/memeImageView1"
		android:layout_width="90dp"
		android:layout_height="160dp"
		android:alpha="0.2"
		android:background="@color/grey"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.763"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.18"
		app:srcCompat="@drawable/ic_add_photo_alternate_24px" />

	<ImageView
		android:id="@+id/memeImageView3"
		android:layout_width="90dp"
		android:layout_height="160dp"
		android:alpha="0.2"
		android:background="@color/grey"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.233"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.18"
		app:srcCompat="@drawable/ic_add_photo_alternate_24px" />

	<ImageView
		android:id="@+id/memeImageView4"
		android:layout_width="90dp"
		android:layout_height="160dp"
		android:alpha="0.2"
		android:background="@color/grey"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.233"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView2"
		app:layout_constraintVertical_bias="0.431"
		app:srcCompat="@drawable/ic_add_photo_alternate_24px" />

	<ImageView
		android:id="@+id/memeImageView5"
		android:layout_width="90dp"
		android:layout_height="160dp"
		android:alpha="0.2"
		android:background="@color/grey"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.763"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView2"
		app:layout_constraintVertical_bias="0.431"
		app:srcCompat="@drawable/ic_add_photo_alternate_24px" />

	<ImageView
		android:id="@+id/memeImageView6"
		android:layout_width="90dp"
		android:layout_height="160dp"
		android:alpha="0.2"
		android:background="@color/grey"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.233"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView2"
		app:layout_constraintVertical_bias="0.786"
		app:srcCompat="@drawable/ic_add_photo_alternate_24px" />

	<ImageView
		android:id="@+id/memeImageView7"
		android:layout_width="90dp"
		android:layout_height="160dp"
		android:alpha="0.2"
		android:background="@color/grey"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.763"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView2"
		app:layout_constraintVertical_bias="0.786"
		app:srcCompat="@drawable/ic_add_photo_alternate_24px" />

	<Button
		android:id="@+id/memeContinue"
		android:layout_width="260dp"
		android:layout_height="wrap_content"
		android:background="@drawable/rounded_rectangle"
		android:text="Continue"
		android:textColor="@color/white"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.496"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.954" />


</androidx.constraintlayout.widget.ConstraintLayout>