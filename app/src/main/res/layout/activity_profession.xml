<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfessionActivity">

	<EditText
		android:id="@+id/professionEditText"
		android:layout_width="260dp"
		android:layout_height="wrap_content"
		android:ems="10"
		android:hint="Your Profession"
		android:inputType="textPersonName"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.103" />

	<EditText
		android:id="@+id/companyEditText2"
		android:layout_width="260dp"
		android:layout_height="wrap_content"
		android:ems="10"
		android:hint="Your Company"
		android:inputType="textPersonName"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/professionEditText"
		app:layout_constraintVertical_bias="0.103" />

	<EditText
		android:id="@+id/univEditText"
		android:layout_width="260dp"
		android:layout_height="wrap_content"
		android:ems="10"
		android:hint="University"
		android:inputType="textPersonName"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/companyEditText2"
		app:layout_constraintVertical_bias="0.103" />

	<Button
		android:id="@+id/professionContinue"
		android:layout_width="260dp"
		android:layout_height="wrap_content"
		android:background="@drawable/rounded_rectangle"
		android:text="Continue"
		android:textColor="@color/white"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/univEditText"
		app:layout_constraintVertical_bias="0.817" />

</androidx.constraintlayout.widget.ConstraintLayout>